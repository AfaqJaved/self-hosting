NODE_ENV=production
LOG_LEVEL=debug
SECRET=your-secret-key

DB_CLIENT=pg
DB_HOST=your-db-host
DB_PORT=5432
DB_USER=your-db-user
DB_PASSWORD=your-db-password
DB_DATABASE=your-db-name

ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=your-admin-password
ADMIN_USERNAME=admin
ADMIN_FIRST_NAME=FirstName
ADMIN_LAST_NAME=LastName

DEFAULT_LANGUAGE=en
DEFAULT_TIMEZONE=UTC
PORT=8055
PUBLIC_URL=https://your-public-url.com

STORAGE_LOCATIONS=s3
STORAGE_S3_DRIVER=s3
STORAGE_S3_KEY=your-s3-key
STORAGE_S3_SECRET=your-s3-secret
STORAGE_S3_ENDPOINT=http://s3-endpoint:9000
STORAGE_S3_BUCKET=your-bucket
STORAGE_S3_REGION=your-region
STORAGE_S3_ROOT=uploads
STORAGE_S3_FORCE_PATH_STYLE=true

NODE_TLS_REJECT_UNAUTHORIZED=0

EMAIL_VERIFY_SETUP=true
EMAIL_FROM=no-reply@example.com
EMAIL_SMTP_HOST=smtp.example.com
EMAIL_SMTP_PORT=587
EMAIL_SMTP_USER=your-smtp-user
EMAIL_SMTP_PASSWORD=your-smtp-password
EMAIL_SMTP_SECURE=false
EMAIL_SMTP_IGNORE_TLS=false
EMAIL_SMTP_NAME=smtp-provider
EMAIL_TRANSPORT=smtp

REDIS_ENABLED=true
REDIS=redis://your-redis-host:6379

CACHE_ENABLED=true
CACHE_AUTO_PURGE=true
CACHE_STORE=redis
